generator client {
  provider = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}
datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}
model Applicant{
    id String @id //provided
    age Int //done
    preferredAges String //json int[] done
    timeZone Int
    preferredTimeZones String //json int[] 
    hobbies String
    gender String
    preferredGenders String //json string[] working on
    aboutYou String
    idealDesc String
    username String
    profile String
    name String //check
}

